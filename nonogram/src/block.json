{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "todays-mitsui/nonogram",
  "version": "0.1.0",
  "title": "Nonogram Puzzle",
	"category": "widgets",
	"icon": "grid-view",
	"description": "Create and solve Nonogram puzzles in the WordPress editor.",
	"keywords": ["nonogram", "picross", "puzzle", "game"],
  "example": {
		"attributes": {
			"aspectRatio": [1, 1],
			"boardData": "v1;5x5;5x5;hKIoiIgiQ"
		}
	},
  "supports": {
		"html": false,
		"align": true,
		"customClassName": true,
		"reusable": true
	},
  "attributes": {
		"aspectRatio": {
			"type": "array",
			"default": [1, 1]
		},
    "boardData": {
      "type": "string",
      "default": null
    }
  },
  "textdomain": "nonogram",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}
